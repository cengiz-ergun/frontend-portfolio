{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            // "@/*": ["./src/*"],
            // "@/public/*": ["./public/*"]
            "@root/*": ["./*"],
            "@popUpGeneratorConstants/*": ["./src/app/popupgenerator/constants/*"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/popupgenerator/context/PaginationCount/PaginationCountContext.tsx"],
    "exclude": ["node_modules"]
}
